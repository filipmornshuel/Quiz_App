<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="240.0" prefWidth="150.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="menu.ControllerMenu">
   <children>
      <VBox layoutX="25.0" layoutY="20.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label alignment="CENTER" prefHeight="17.0" prefWidth="108.0" text="Quiz App" />
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#startButton_Clicked" prefWidth="80.0" text="Start" />
                  <Button mnemonicParsing="false" onAction="#scoreButton_Clicked" prefWidth="80.0" text="Score" />
                  <Button mnemonicParsing="false" onAction="#quitButton_Clicked" prefWidth="80.0" text="Quit" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
